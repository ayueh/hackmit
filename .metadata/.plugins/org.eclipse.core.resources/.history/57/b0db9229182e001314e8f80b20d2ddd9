package com.example.vibe;


public class concatenatearr{
	private double arr[];
	
	public concatenatearr(double Data[], int index){
		arr = new double[Data.length];
		System.Arraycopy(Data, index, arr, 0, Data.length-index);
		System.Arraycopy(Data, 0, arr, Data.length-index, index);
	}
	
	public void printVals(){
		for(int i = 0; i<(arr.length); i++)
		{
			System.out.printf("%f, ", arr[i]);
		}
	}
	
	public double[] getValues(){
		return arr;
	}
}